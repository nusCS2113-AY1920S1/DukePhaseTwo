<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity" prefWidth="400.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <VBox fx:id="dialogHolder" alignment="TOP_RIGHT" prefHeight="44.0" prefWidth="314.0">
        <HBox.margin>
            <Insets left="10.0" right="10.0" top="3.0"/>
        </HBox.margin>
        <Text fx:id="dialog" text="Label"/>
    </VBox>
    <Circle fx:id="displayPicture" fill="WHITE" radius="22.0" stroke="BLACK" strokeType="INSIDE">
        <HBox.margin>
            <Insets/>
        </HBox.margin>
    </Circle>
</fx:root>
