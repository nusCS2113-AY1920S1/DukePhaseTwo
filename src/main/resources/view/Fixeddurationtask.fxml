<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="390.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFx.FixedDurationTask">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                        <MenuItem mnemonicParsing="false" text="Open…" />
                        <Menu mnemonicParsing="false" text="Open Recent" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                        <MenuItem mnemonicParsing="false" text="Save" />
                        <MenuItem mnemonicParsing="false" text="Save As…" />
                        <MenuItem mnemonicParsing="false" text="Revert" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Preferences…" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Cut" />
                        <MenuItem mnemonicParsing="false" text="Copy" />
                        <MenuItem mnemonicParsing="false" text="Paste" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Select All" />
                        <MenuItem mnemonicParsing="false" text="Unselect All" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane prefHeight="358.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
            <children>
                <AnchorPane layoutX="27.0" layoutY="258.0" prefHeight="47.0" prefWidth="105.0">
                    <children>
                        <Label id="taskTypeLabel" layoutX="4.0" layoutY="-2.0" prefHeight="21.0" prefWidth="78.0" text="Task Type" />
                        <ChoiceBox fx:id="taskTypeChoiceBox" layoutY="20.0" prefHeight="27.0" prefWidth="105.0" />
                    </children>
                </AnchorPane>
                <VBox layoutX="27.0" layoutY="18.0" prefHeight="207.0" prefWidth="566.0">
                    <children>
                        <Label id="availablePeriodLabel" text="Please choose a available Period" />
                        <ListView fx:id="periodListView" onKeyPressed="#updatePeriod" onMouseClicked="#updatePeriod" prefHeight="188.0" prefWidth="566.0" />
                    </children>
                </VBox>
                <AnchorPane layoutX="535.0" layoutY="245.0">
                    <children>
                        <Button fx:id="addButton" layoutX="1.0" layoutY="33.0" mnemonicParsing="false" onAction="#handleAdd" onKeyPressed="#handleAdd" prefHeight="27.0" prefWidth="61.0" text="Add" />
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancel" onKeyPressed="#handleCancel" prefHeight="27.0" prefWidth="61.0" text="Cancel" />
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="153.0" layoutY="260.0" prefHeight="45.0" prefWidth="187.0">
                    <children>
                        <TextField fx:id="taskDescriptionTextField" layoutX="-7.0" layoutY="18.0" prefHeight="27.0" prefWidth="202.0" promptText="Please enter Task Description" />
                        <Label id="taskDescriptionLabel" text="Type Description" />
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="359.0" layoutY="260.0">
                    <children>
                        <Label id="periodLabel" layoutX="2.0" text="Period" />
                        <TextField fx:id="periodTextField" layoutY="19.0" promptText="Selected from list" />
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</VBox>